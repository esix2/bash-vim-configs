bindsym XF86AudioRaiseVolume exec "~/.config/sway/increasevolume"
bindsym XF86AudioLowerVolume exec pactl set-sink-volume @DEFAULT_SINK@ -5%
bindsym XF86AudioMute exec pactl set-sink-mute @DEFAULT_SINK@ toggle
bindsym XF86AudioMicMute exec pactl set-source-mute @DEFAULT_SOURCE@ toggle
bindsym $mod+XF86AudioMute exec pactl set-source-mute @DEFAULT_SOURCE@ toggle
#bindsym $mod+XF86AudioRaiseVolume exec "pactl set-source-mute 0 false ; pactl set-source-volume @DEFAULT_SOURCE@ +5%"
bindsym $mod+XF86AudioRaiseVolume exec "~/.config/sway/increasemic"
bindsym $mod+XF86AudioLowerVolume exec pactl set-source-volume @DEFAULT_SOURCE@ -5%

